{
  "expo": {
    "name": "CamelliaAppFrontendJS",
    "slug": "CamelliaAppFrontendJS",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera to function properly.",
        "NSLocationWhenInUseUsageDescription": "This app uses your location to provide better services."
      }
    },
    "android": {
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "CAMERA"
      ],
      "package": "com.anonymous.CamelliaAppFrontendJS"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
